<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle" >
            <gradient
                android:startColor="#FEE500"
                android:endColor="#FEE500"
                android:type="linear"
                />

            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp"
                />
            <size
                android:width="50dp"
                android:height="50dp"
                />
            <corners
                android:radius="30dp"
                />
        </shape>
    </item>
    <item android:drawable="@drawable/kakao_login_symbol"
        android:top="2dp"
        android:width="25dp"
        android:height="25dp"
        android:gravity="center"
        >
    </item>

</layer-list>




